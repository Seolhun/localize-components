{
  "name": "@seolhun/localize-react-components",
  "version": "1.0.0",
  "description": "just pursue simple and easy react components to customize.",
  "main": "dist/src/index.js",
  "scripts": {
    "prepublish": "yarn style && tsc",
    "style": "copyfiles src/**/*.scss dist/",
    "test": "jest",
    "build": "cd docs && node build/build.js",
    "dev": "cd docs && node build/dev-server.js",
    "clean": "lerna clean",
    "packages:fix": "lerna run fix && echo",
    "packages:lint": "lerna run tslint",
    "packages:build": "lerna run build --ignore docz-example-*",
    "prerelease": "yarn run packages",
    "release": "lerna publish --conventional-commits"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Seolhun/localize-react-components"
  },
  "keywords": [
    "react",
    "typescript",
    "component",
    "ui",
    "scss"
  ],
  "author": "Seolhun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Seolhun/localize-react-components/issues"
  },
  "homepage": "https://github.com/Seolhun/localize-react-components#readme",
  "dependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.2"
  },
  "devDependencies": {
    "@types/react": "16.4.7",
    "@types/react-dom": "16.0.6",
    "copyfiles": "2.0.0",
    "lerna": "3.1.1",
    "tslint-config-airbnb": "5.9.2",
    "typescript": "3.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  }
}
