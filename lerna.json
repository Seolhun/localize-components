{
  "version": "0.5.3",
  "packages": [
    "packages/*"
  ],
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "command": {
    "publish": {
      "allowBranch": [
        "master",
        "develop"
      ],
      "ignoreChanges": [
        "**/test",
        "**/*.md"
      ]
    }
  },
  "changelog": {
    "labels": {
      "nextVersion": "Unreleased",
      "feature": "New Feature",
      "bug": "Bug Fix"
    }
  }
}
